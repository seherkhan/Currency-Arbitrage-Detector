<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane maxHeight="600.0" maxWidth="1200.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.Screen2Controller">
   <children>
      <ScrollPane layoutY="58.0" maxHeight="520.0" maxWidth="1200.0">
         <content>
            <GridPane fx:id="gridPane">
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" />
                <ColumnConstraints hgrow="SOMETIMES" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints maxHeight="30.0" minHeight="30.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
            </GridPane>
         </content>
      </ScrollPane>
      <Button fx:id="selectAll" layoutX="14.0" layoutY="33.0" mnemonicParsing="false" onAction="#selectAllPressed" prefHeight="25.0" prefWidth="82.0" text="Select All" />
      <Button fx:id="deselectAll" layoutX="96.0" layoutY="33.0" mnemonicParsing="false" onAction="#deselectAllPressed" prefHeight="25.0" prefWidth="82.0" text="Deselect All" />
      <Button fx:id="create" layoutX="318.0" layoutY="33.0" mnemonicParsing="false" onAction="#createPressed" prefHeight="25.0" prefWidth="90.0" text="Create Graph" />
      <Button fx:id="restart" layoutX="228.0" layoutY="33.0" mnemonicParsing="false" onAction="#restartPressed" prefHeight="25.0" prefWidth="90.0" text="Restart" />
      <Label layoutX="20.0" layoutY="12.0" text="Uncheck boxes to remove edges" />
   </children>
</AnchorPane>
